using System;
public class ifElse
{

    public static void Main(String[] args)
    {
        Console.WriteLine("Enter the number to Test");
        int i=Convert.ToInt32(Console.ReadLine());
        if(i%2==0)
        {
            Console.WriteLine("It is Even number");
        }
        else
        {
            Console.WriteLine("It is Odd number");
        }
        Console.ReadLine();
    }
}
