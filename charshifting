using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            string correct = "";
            correct = Console.ReadLine();
            
            for(int i=0; i<correct.Length; i++)
               {
                   char c= correct[i];
                   c++;
                  Console.Write(c); 
               }
            Console.ReadLine();
        }
    }
}
