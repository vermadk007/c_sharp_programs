using System;
public class breakNestedForLoop
{

    public static void Main(String[] args)
    {
        for(int i=1;i<=10;i++)
        {
            for(int j=1;j<=5;j++)
            {
                if(j==3)
                break;
                Console.WriteLine(i+"  "+j);
            }
        }
      
        Console.ReadLine();
    }
}
