using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
	class Program
	{
		static void Main(string[] args)
		{
		//nested for Loop
        Console.WriteLine("i  j");
        for(int i=1;i<=3;i++)
        {
            for(int j=1;j<=5;j++)
            {
                Console.WriteLine(i+ "  "+j);
            }
        }
        Console.ReadLine();
		}
	}
}
