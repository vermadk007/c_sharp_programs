using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace SoloLearn
{
	class Program
	{
		static void Main(string[] args)
		{
		//Nested While loop
       int i=1;
        Console.WriteLine("i  j");
        while(i<=2)
        {
            int j=1;
            while(j<=3)
            {
                Console.WriteLine(i+"  "+j);
                j++;
            }
            i++;
        }
        Console.ReadLine();
		}
	}
}
